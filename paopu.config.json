{
  "helper": {
    "module": false,
    "resourceBasePath": "test",
    "resources": ["helper.js"],
    "targets": ["test/doc.md", "test/tooltip/doc.md"],
    "urlPattern": "unpkg.com"
  },
  "upgraded-element": {
    "resources": ["dist/upgraded-element.js", "dist/upgraded-element.min.js"],
    "targets": ["test/upgraded-element/doc.md"]
  },
  "@spider-ui/tooltip": {
    "resources": ["dist/tooltip.js", "dist/tooltip.min.js"],
    "targets": ["test/tooltip/doc.md"]
  },
  "no-resources": {
    "targets": ["some/path/doc.md"]
  },
  "no-targets": {
    "resources": ["some/path/doc.md"]
  },
  "missing-resource-base-path": {
    "module": false,
    "resources": ["dist/tooltip.js", "dist/tooltip.min.js"],
    "targets": ["test/tooltip/doc.md"]
  },
  "no-resource-base-path-for-module": {
    "resourceBasePath": "some/resource/path",
    "resources": ["dist/tooltip.js", "dist/tooltip.min.js"],
    "targets": ["test/tooltip/doc.md"]
  }
}
